<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="274.0" prefWidth="491.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <Text layoutX="44.0" layoutY="56.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please confirm deletion of player before deleting from database." />
        <Text layoutX="111.0" layoutY="189.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Are you sure you want to delete this player?" />
        <Button fx:id="deleteYes" layoutX="165.0" layoutY="217.0" mnemonicParsing="false" onAction="#delete" text="Yes" />
        <Text layoutX="124.0" layoutY="119.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Player Name " />
        <TextField fx:id="getPlayerName" layoutX="215.0" layoutY="101.0" onAction="#delete" text="Enter player name" />
        <Button fx:id="deleteNow" layoutX="269.0" layoutY="217.0" mnemonicParsing="false" onAction="#CloseDeletePlayer" text="Close" />
    </children>
</AnchorPane>